{
    "version": 2,
    "cmakeMinimumRequired": {
        "major": 3,
        "minor": 22,
        "patch": 0
    },
    "configurePresets": [
    {
        "name": "AC6",
        "displayName": "AC6",
        "description": "Static Library with AC6",
        "generator": "Ninja",
        "environment": {
            "CORTEX_TYPE": "cm7"
        },
        "binaryDir": "${sourceDir}/build/${presetName}",
        "cacheVariables": {
            "FETCHCONTENT_UPDATES_DISCONNECTED": "OFF",
            "USE_CMSIS_DSP": true,
            "CMAKE_BUILD_TYPE": "Debug",
            "CMAKE_INSTALL_PREFIX": "${sourceDir}/install/${presetName}",
            "CMAKE_TOOLCHAIN_FILE": "${sourceDir}/build/${presetName}/_deps/cmake-toolchains-src/armclang-community.cmake",
            "GITHUB_BRANCH_TOOLCHAIN": "HEAD"
        }
    },
    {
        "name": "GCC",
        "displayName": "GCC",
        "description": "Static library with GCC",
        "generator": "Ninja",
        "environment": {
            "CORTEX_TYPE": "cm7",
            "ARM_GCC_ROOT_FOLDER": "/Users/sohal/toolchains/gcc-arm-none-eabi-10.3-2021.10"
        },
        "binaryDir": "${sourceDir}/build/${presetName}",
        "cacheVariables": {
            "FETCHCONTENT_UPDATES_DISCONNECTED": "OFF",
            "USE_CMSIS_DSP": true,
            "CMAKE_BUILD_TYPE": "Debug",
            "CMAKE_INSTALL_PREFIX": "${sourceDir}/install/${presetName}",
            "CMAKE_TOOLCHAIN_FILE": "${sourceDir}/build/${presetName}/_deps/cmake-toolchains-src/arm-none-eabi-gcc.cmake",
            "GITHUB_BRANCH_TOOLCHAIN": "HEAD"
        }
    }
    ],
    "buildPresets": [
        {
            "name": "AC6",
            "configurePreset": "AC6"
        },
        {
            "name": "GCC",
            "configurePreset": "GCC"
        }
    ]
}
